{
  "name": "rev-mobile-test",
  "version": "0.0.1",
  "description": "Mobile web for resolving clothing",
  "main": "index.js",
  "scripts": {
    "start": "cross-env HAPPY_CACHE=true npm run watch",
    "test": "npm run karma:single & npm run casperjs:suite",
    "build": "cross-env HAPPY_VERBOSE=true npm run webpack:mobile-app & npm run webpack:prod",
    "clean": "npm run clean:dist && rimraf lib/scss/sprite coverage node_modules",
    "clean:dist": "rimraf dist",
    "lint": "npm run eslint",
    "watch": "webpack --config webpack/webpack.build.config.js --display-chunks --display-modules --watch --expand-css --progress",
    "watch:test": "npm run karma",
    "webpack:prod": "webpack --config webpack/webpack.production.config.js --display-chunks --display-modules --progress",
    "webpack:mobile-app": "webpack --config webpack/webpack.mobile-app.config.js --display-chunks --display-modules",
    "webpack-dev-server": "cross-env HAPPY_CACHE=true webpack-dev-server --config webpack/webpack.development.config.js --expand-css",
    "karma": "karma start",
    "karma:single": "npm run karma -- --single-run=true --browsers=PhantomJS --no-colors",
    "casperjs": "./bin/casperjs",
    "casperjs:suite": "npm run casperjs -- --files 'test/integration/**/*.test.js'",
    "eslint": "eslint WebRoot/r/mobile/scripts test/unit",
    "svg:enable": "./bin/svg-enable",
    "svg:disable": "./bin/svg-disable"
  },
  "repository": {
    "type": "git",
    "url": "git@ext-git.dressbad.com:firesqk"
  },
  "author": "",
  "license": "UNLICENSED",
